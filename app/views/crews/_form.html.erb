<%= form_for crew do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :rank %>
    <%= f.text_field :rank %>
    <br>
    <%= f.label :specialty %>
    <%= f.text_field :specialty %>
    <br>
    <%= f.label :clearance, "Security Clearance?" %>
    <br>
    <%= f.radio_button :clearance, "false" %>
    <%= f.label :clearance_false, "No" %>
    <br>
    <%= f.radio_button :clearance, "true" %>
    <%= f.label :clearance_true, "Yes" %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <br>
    <%= f.submit %>
<% end %>